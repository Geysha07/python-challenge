import os
import csv

csvpath=os.path.join('Resources', 'budget_data.csv')
print(csvpath)

with open(csvpath) as csvfile:
    csvreader=csv.reader(csvfile,delimiter=',')
    print(csvreader)

    header = next(csvreader)
    print(f"Header: {header}")
    for row in csvreader:
        print('Financial Analysis')
        print('----------------------------')
        print(row)